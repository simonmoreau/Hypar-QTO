{
  "$schema": "https://hypar.io/schemas/function.json",
  "description": "A generator for building a simplified model for Quantity TakeOff.",
  "id": "bim42-hypar-qto",
  "name": "Hypar QTO",
	"inputs": [
		{
			"name": "Headroom",
			"min": 2.0,
			"max": 5.0,
			"step": 0.1,
			"description": "The headroom in a level.",
			"type": "range"
		},
		{
			"name": "Width",
			"min": 10.0,
			"max": 30.0,
			"step": 1.0,
			"description": "The width of the building",
			"type": "range"
		},
		{
			"name": "Module Width",
			"min": 1.0,
			"max": 2.0,
			"step": 0.05,
			"description": "The width of a fa√ßade element",
			"type": "range"
		}
	],
	"outputs": [
		{
			"name": "area",
			"description": "The area of the mass.",
			"type": "number"
		}
	],
  "repository_url": "https://github.com/simonmoreau/Hypar-QTO"
}
